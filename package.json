{
  "name": "ranks",
  "version": "0.2.2",
  "main": "src/index.js",
  "license": "MIT",
  "repository": "soyjavi/ranks",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron ./src/index",
    "forge": "NODE_ENV=development electron-forge start",
    "package": "electron-forge package",
    "lint": "eslint src/**"
  },
  "config": {
    "forge": {
      "make_targets": {
        "darwin": [
          "zip"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn",
        "icon": "./build/assets/app-icon.png"
      },
      "electronInstallerDMG": {
        "format": "ULFO",
        "icon": "./build/assets/app-icon.png"
      }
    }
  },
  "dependencies": {
    "auto-launch": "^5.0.5",
    "electron-compile": "^6.4.2",
    "prop-types": "^15.6.1",
    "react": "next",
    "react-dom": "next",
    "react-native-web": "^0.5.3",
    "seed-color": "^2.0.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-eslint": "8.2.1",
    "babel-plugin-react-native-web": "^0.5.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react-native": "^4.0.0",
    "cross-env": "^5.1.4",
    "electron": "^1.8.4",
    "electron-forge": "^5.2.0",
    "electron-prebuilt-compile": "1.8.4",
    "electron-reload": "^1.2.2",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0"
  }
}
